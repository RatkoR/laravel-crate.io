{
    "name": "ratkor/laravel-crate.io",
    "description": "Crate.io driver for Laravel 5",
    "license": "MIT",
    "keywords" : ["laravel5","laravel","crate.io","crate","pdo_crate"],
    "authors": [
        {
            "name": "Ratko Rudic",
            "email": "ratko@kramfid.org"
        }
    ],
    "require": {
        "php": ">7.2",
        "ext-pdo":  "*",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "crate/crate-dbal": "~1.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.8.0|^4.0"
    },
    "autoload": {
        "psr-0": {
            "RatkoR\\Crate": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RatkoR\\Crate\\CrateServiceProvider"
            ]
        }
    }
}
